    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "jest": "16.0.1",
    "karma": "^1.3.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "nightwatch": "^0.9.9",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "sitemap-webpack-plugin": "^0.1.1",
    "superagent": "^3.3.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/tests/preprocessor",
    "globals": {
      "__DEV__": true,
      "__PROD__": false,
      "__DEBUG__": false,
      "__COVERAGE__": false,
      "__PROTOCOL__": false,
      "__HOST__": false,
      "__PORT__": false
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "css",
      "sss",
      "md"
    ],
    "moduleDirectories": [
      "node_modules",
      "universal",
      "client"
    ],
    "moduleNameMapper": {
      "^.+\\.(md|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "(/blueprints/.*|\\.md$)"
    ]
  }
}
